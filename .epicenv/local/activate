if [ -n "${EPICENV}" ]; then
    echo deactivating $EPICENV
    epic-deactivate
fi
temp_src=$(epicenv zzz_INTERNAL_gen -e local)
if [ $? -lt 1 ]; then
	echo activated env local
    source $temp_src
    if [ -n "${EPICENV_DEV}" ]; then
        echo leaving temp file $temp_src for debug
    else
        rm $temp_src
    fi
fi
